<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.cqupt.service.dao.mapper.UserMapper" >

  <select id="login" resultType="com.cqupt.service.dao.model.User" parameterType="java.lang.Integer" >
